{
  "name": "@kenx/framework",
  "version": "1.0.0",
  "description": "A modern, lightweight Node.js web framework with TypeScript support, inspired by Express and Hapi",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node examples/fullstack-app.ts",
    "dev:basic": "ts-node examples/basic-app.ts",
    "dev:fullstack": "ts-node examples/fullstack-app.ts",
    "start": "node dist/examples/basic-app.js",
    "watch": "nodemon --exec ts-node examples/basic-app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts examples/**/*.ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "web",
    "framework",
    "http",
    "server",
    "typescript",
    "nodejs",
    "express",
    "hapi",
    "middleware",
    "routing"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "commonjs",
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@types/ws": "^8.18.1",
    "ejs": "^3.1.10",
    "handlebars": "^4.7.8",
    "uuid": "^11.1.0",
    "ws": "^8.18.0"
  }
}
